<UserControl x:Class="Examen_Galaxy.View.ImportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Examen_Galaxy.View" xmlns:viewmodel="clr-namespace:Examen_Galaxy.ViewModel" d:DataContext="{d:DesignInstance Type=viewmodel:ImportViewModel}"
             mc:Ignorable="d" 
             Background="Yellow"
             d:DesignHeight="450" d:DesignWidth="800"
  
             >
    <Grid>
        <Button Content="Importar datos" HorizontalAlignment="Center" Margin="0,188,0,0" Command="{Binding LoadFromFileCommand}" VerticalAlignment="Top" Height="65" Width="204"/>

    </Grid>
</UserControl>
